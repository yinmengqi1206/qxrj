/* pages/index/index.wxss */
.header {
  position: sticky;
  top: 0;
  z-index: 10;
}

.back-icon {
  width: 48rpx;
  height: 48rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.index-icon{
  margin: 0 5px;
  width: 40px;
  height: 40px;
}
.calendar-overlay {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 1000;
}